{
	"entries": [
		"./assembly/bson.ts"
	  ],
	"targets": {
		"debug": {
			"binaryFile": "build/untouched.wasm",
			"textFile": "build/untouched.wat",
			"sourceMap": true,
			"debug": true
		},
		"release": {
			"binaryFile": "build/optimized.wasm",
			"textFile": "build/optimized.wat",
			"sourceMap": true,
			"optimizeLevel": 3,
			"shrinkLevel": 1,
			"converge": true,
			"noAssert": false
		}
	},
	"options": {
		"exportRuntime": true
	}
}
